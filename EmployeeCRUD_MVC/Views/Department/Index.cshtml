 @model IEnumerable<EmployeeCRUD_MVC.Models.Department>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Did)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Did)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DName)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new {  did=item.Did  }) |
                    @Html.ActionLink("Details", "Details", new { did=item.Did }) |
                    @Html.ActionLink("Delete", "Delete", new { did=item.Did })
            </td>
        </tr>
}
    </tbody>
</table>
